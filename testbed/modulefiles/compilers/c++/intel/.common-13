#%Module1.0

# Software description
module-whatis   "Intel C++ compiler"
set software    "c++/intel"
set description "Intel C++ compiler (icpc)"

# Conflict
#conflict        "c++"

# Loads software's environment
# compilerwrappers-specific variables
set cxx         "icpc"
#set mpicxx      ""

# Common
if [file exists $env(MODULEPATH)/compilers/c/intel/.common-13-ccpp] {
    source $env(MODULEPATH)/compilers/c/intel/.common-13-ccpp
}

