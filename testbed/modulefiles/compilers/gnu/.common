#%Module1.0

# Software description
module-whatis   "GNU Compiler Collection"
set software    "gnu"
set description "GNU C, C++ and Fortran compilers"
set html        "http://gcc.gnu.org/"

# Conflict
#conflict        "intel"
conflict        "$software"

# Load underlying softwares
module load c/gnu/$version
module load c++/gnu/$version
module load fortran/gnu/$version

# load common functions and behavior
source $env(MODULEPATH)/.common

