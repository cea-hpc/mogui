#%Module1.0

# Software description
module-whatis   "Bull Simulator"
set software    "simubull"
set version     "0.0.1"
set description "Use the bull simulator to run VM instead of the standard QEMU"
set html        ""

# Conflict
conflict        "$software"

set appnameup "SIMUBULL"

setenv QEMU_BIN "qemu-simu"
setenv VM_NIC_BULL "1"

# load common functions and behavior
source $env(MODULEPATH)/.common

